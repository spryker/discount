<div class="ibox nested query-builder-info" data-auto-errors="true">
    <div class="ibox-title">
        <a class="collapse-link">
            <h5>
                <span class="fa fa-question-circle" aria-hidden="true"></span> {{ 'Examples' | trans }}
            </h5>
            <div class="ibox-tools">
                <i class="fa fa-chevron-up"></i>
            </div>
        </a>
    </div>
    <div class="ibox-content" style="display: none;">
        <table class="table table-striped table-bordered">
            <tr>
                <th>{{ 'Field' | trans }}</th>
                <th>{{ 'Operation' | trans }}</th>
                <th>{{ 'Value' | trans }}</th>
                <th>{{ 'Explanation' | trans }}</th>
            </tr>
            <tr>
                <td>sku</td><td>equal</td><td>SKU-1</td><td>{{ 'Matches elements with the exact same SKU: "SKU-1"' | trans }}</td>
            </tr>
            <tr>
                <td>attribute.width</td><td>equal</td><td>*</td><td>Matches all elements which have a "width" attribute defined, but with any "width" value</td>
            </tr>
            <tr>
                <td>sku</td><td>in</td><td>SKU-1</td><td>Matches elements with the exact same SKU: "SKU-1"</td>
            </tr>
            <tr>
                <td>sku</td><td>in</td><td>SKU-1;SKU-2</td><td>Matches elements with the exact same SKU as "SKU-1" or "SKU-2"</td>
            </tr>
            <tr>
                <td>attribute.width</td><td>in</td><td>*</td><td>Matches all elements which have a "width" attribute defined, but with any "width" value</td>
            </tr>
        </table>
    </div>
</div>
